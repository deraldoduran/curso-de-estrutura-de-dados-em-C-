# include <iostream>
using namespace std;

int piramide (int *v,int n){
    int cont=0;
    //cont =cont+ piramide (v,n-1)+piramide(v,n-2);
    if(n==1){
         cout<<cont<<endl;
        return cont;
    } //cont //piramide(v,n-1)+piramide(v,n-2);
    else {
       cont =cont + piramide(v,n-1)+piramide(v,n-2);
       cout<<cont<<endl;
        return cont + piramide(v,n-1)+piramide(v,n-2);
    }
   // cout<<piramide(v,n);
}

int main(){
    int n;
    cin>>n;
    int vet[n];
    for(int i=0; i<n;i++){
        cin>>vet[i];
        //cout<<vet[i]<<endl;
    }
    piramide(vet, n);
    
    //cout<<piramide(vet, n)<<endl;
    
    return 0;
}
