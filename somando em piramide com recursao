# include <iostream>
using namespace std;

int piramide (int *v,int n){
    int cont=5;
    //cont =cont+ piramide (v,n-1)+piramide(v,n-2);
    if(n==1){
        // cout<<cont<<endl;
        return piramide(v,n-1);
    } 
    else {
       //cont =cont + piramide(v,n-1)+piramide(v,n-2);
       cout<<piramide(v,n-1)+piramide(v,n-2)<<endl;
        return piramide(v,n-1)+piramide(v,n-2);
    }
   // cout<<piramide(v,n);
}

int main(){
    int n;
    cin>>n;
    int vet[n];
    for(int i=0; i<n;i++){
        cin>>vet[i];
        //cout<<vet[i]<<endl;
    }
    piramide(vet, n);
    
    //cout<<piramide(vet, n)<<endl;
    
    return 0;
}
